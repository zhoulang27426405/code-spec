{
  "name": "code-spec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tiny": "node tinypng --all",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "vuedoc": "vuese gen && vuese serve --open",
    "jsdoc": "jsdoc -c jsdoc.json",
    "lint": "vue-cli-service lint",
    "format": "eslint --fix --ignore-path .gitignore . --ignore-pattern \"/jsdocs/*\" . && prettier --write \"**/*.{less,scss}\""
  },
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "docdash": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "husky": "^1.3.1",
    "jsdoc": "^3.6.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "url": "^0.11.0",
    "vue-template-compiler": "^2.5.21",
    "vuese": "^1.4.7"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ignore-pattern '/jsdocs/*'",
      "git add"
    ],
    "*.{less,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg}": [
      "node tinypng",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
